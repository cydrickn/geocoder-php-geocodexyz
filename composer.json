{
    "name": "cydrickn/geocoder-php-geocodexyz",
    "description": "Geocode.xyz Provider",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "cydrickn",
        "geocoder",
        "geocodexyz",
        "geocode",
        "geocode.xyz",
        "xyz",
        "provider",
        "location"
    ],
    "authors": [
        {
            "name": "Cydrick Nonog",
            "email": "cydrick.dev@gmail.com"
        }
    ],
    "require": {
        "php": "^7.0",
        "willdurand/geocoder": "^4.0",
        "geocoder-php/common-http": "^4.0"
    },
    "require-dev": {
        "phpunit/phpunit": "6.3.*",
        "geocoder-php/provider-integration-tests": "^1.0",
        "php-http/message": "^1.0",
        "php-http/curl-client": "^1.7",
        "nyholm/psr7": "^0.2.2"
    },
    "provide": {
        "geocoder-php/provider-implementation": "1.0"
    },
    "autoload": {
        "psr-4": { "Cydrickn\\Geocoder\\Provider\\GeocodeXyz\\": "" },
        "exclude-from-classmap": [
            "/Tests/"
        ]
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-ci": "vendor/bin/phpunit --coverage-text --coverage-clover=build/coverage.xml"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
